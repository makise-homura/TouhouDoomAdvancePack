ACTOR ScoreItem10 : GenericScoreItem
{
	Inventory.Amount 10
	States
	{
		Spawn:
			POIN A 0 // We should have an empty line here for some reason
			POIN A 600
			POIN AAAAAAAAAA 3 A_FadeOut
