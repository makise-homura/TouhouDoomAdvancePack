ACTOR MarisaBlazingStarHB : MarisaBlazingStar replaces MarisaBlazingStar
{
    Tag "Marisa Kirisame"
    States
    {
        See: // Well, it's survival spell card
            MARI F 0 ACS_NamedExecute("BHI_BossStartSurvivalAttack",0,1)
            goto Super::See
        Death:
        XDeath:
            MARI F 0 ACS_NamedTerminate("BHI_BossStartSurvivalAttack",0)
            MARI F 0 ACS_NamedExecute("BHI_BossStopAttack",0)
            MARI F 0 A_SpawnItem("LittleSpellCardBonus")
            MAR2 X 1 // Note another death sprite
            goto Super::Death
    }
}
