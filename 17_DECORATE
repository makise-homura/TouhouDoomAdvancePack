            RANY F 0 ACS_NamedExecute("BHI_BossStopAttack",0)
            RANY C 1
            // Fix the issue that Ran gets spell break at NONspell.
            // UPD: Seems to be already fixed.
            /* RANY C 0 A_GiveInventory("BulletDeathTrigger",1)
            RANY C 0 A_NoBlocking
            RANY C 0 A_PlaySound("touhou/nuefire")
            RANY CCCCCCCCCC 2 A_FadeOut(0.1)
            Stop */
            goto Super::Death
    }
}
