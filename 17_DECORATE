/*****************************************
  DECORATE ENTRY FOR THIRD EPISODE BOSSES
 *****************************************/
 
// NB. If you see something like "We should have an empty line here for some reason",
// then it means we really should. I don't know why, but without it following lines
// became skipped without any clear meaning, at least in Zandronum 3.0. Just bugs.

// Note: one frame in boss's death state is designed to separate call of 
// BHI_BossStopAttack from BHI_BossStartAttack of next spell/nonspell. Otherwise
// they may conflict. It is not required after last spell, of course.

/**** SANAE ****/

ACTOR SanaeHB : Sanae replaces Sanae
{
    Tag "Sanae Kochiya"
    States
    {
        See:
            SANA F 0 ACS_NamedExecute("BHI_BossStartAttack",0,15,ACS_NamedExecuteWithResult("BHI_SpellCardsBySkillLevel",4433),false)
            goto Super::See
        Death:
            SANA F 0 ACS_NamedTerminate("BHI_BossStartAttack",0)
            SANA F 0 ACS_NamedExecute("BHI_BossStopAttack",0)
            SANA L 1
            goto Super::Death
    }
}
ACTOR SanaeGreyThaumaturgyHB : SanaeGreyThaumaturgy replaces SanaeGreyThaumaturgy
{
    Tag "Sanae Kochiya"
    States
    {
        See:
            SANA F 0 Thing_ChangeTID(0,6001)
            SANA F 0 ACS_NamedExecute("BHI_BossStartAttack",0,6001,ACS_NamedExecuteWithResult("BHI_SpellCardsBySkillLevel",3322),true)
            goto Super::See
        Death:
            SANA F 0 ACS_NamedTerminate("BHI_BossStartAttack",0)
            SANA F 0 ACS_NamedExecute("BHI_BossStopAttack",0)
            SANA F 0 A_SpawnItem("LittleSpellCardBonus")
            SANA L 1
            goto Super::Death
    }
}
